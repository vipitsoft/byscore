@{
    Layout = "_Layout1";
    ViewData["Title"] = "日志详情";
}
@model ApplicationLog
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            日志详情
        </h1>
        <ol class="breadcrumb">
            <li><a href="/home/index"><i class="fa fa-home"></i> 首页</a></li>
            <li><a href="/applog/index">日志列表</a></li>
            <li class="active">日志详情</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="box box-primary">

                    <!-- /.box-header -->

                    <div class="box-body">
                        <div class="form-group">
                            <label for="Name">应用名：</label>
                            <label>@Model.Application</label>
                        </div>
                        <div class="form-group">
                            <label for="Price">时间：</label>
                            <label>@Model.Logged.ToString("yyyy-MM-dd HH:mm:ss")</label>
                        </div>
                        <div class="form-group">
                            <label for="Discout">等级：</label>
                            <label>@Model.Level</label>
                        </div>
                        <div class="form-group">
                            <label for="Description">消息：</label>
                            <label>@Model.Message</label>
                        </div>
                        <div class="form-group">
                            <label for="Description">异常信息：</label>
                            <label>@Model.Exception</label>
                        </div>
                    </div>



                </div>
                <!-- /.box -->

            </div>
            <!--/.col (left) -->

        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
